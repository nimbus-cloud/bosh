check process job_1_with_pre_start_script
  with pidfile /var/vcap/sys/run/job_1_with_pre_start_script.pid
  start program "/var/vcap/jobs/job_1_with_pre_start_script/bin/job_1_with_pre_start_script_ctl start"
  stop program "/var/vcap/jobs/job_1_with_pre_start_script/bin/job_1_with_pre_start_script_ctl stop"
  group vcap